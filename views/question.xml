<QuestionForm xmlns="http://mechanicalturk.amazonaws.com/AWSMechanicalTurkDataSchemas/2005-10-01/QuestionForm.xsd">
  <Question>
    <QuestionIdentifier><%= id %></QuestionIdentifier>
    <DisplayName><%= questionname %></DisplayName>
    <IsRequired>true</IsRequired>
    <QuestionContent>
      <Text>How much would someone pay for the item in the picture? (In USD)</Text>
      <Binary>
        <MimeType>
          <Type>image</Type>
        </MimeType>
        <DataURL><%= imageurl %></DataURL>
        <AltText>Please value the item in the picture</AltText>
      </Binary>
    </QuestionContent>
    <AnswerSpecification><FreeTextAnswer/></AnswerSpecification>
  </Question>
</QuestionForm>
